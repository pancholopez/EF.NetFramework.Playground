		     connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=cinteraDB;Integrated Security=SSPI;App=playground"
		     providerName="System.Data.SqlClient" />
		<add name="localDBFile"
		     connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=cinteraDB;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\CinteraDB.mdf;App=playground"
		     providerName="System.Data.SqlClient" />
